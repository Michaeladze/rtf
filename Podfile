

target 'RTF' do
platform :ios, '10.0'
  use_frameworks!
  inhibit_all_warnings!

  # Redux pods
  pod 'ReSwift', '~> 4.0'
  pod 'ReSwiftThunk'
  pod 'ReactiveReSwift'

  # Reactive pods
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5'
  pod 'RxKeyboard'

  # Networking pods
  pod 'Nuke', '~> 7.4'

  # Diffing pods
  pod 'DifferenceKit/UIKitExtension'

  pod 'Alamofire'



end